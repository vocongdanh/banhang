# Database
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=ai_platform
DB_USERNAME=root
DB_PASSWORD=root

# Redis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

# Qdrant
QDRANT_HOST=qdrant
QDRANT_PORT=6333

# Python Service
PYTHON_SERVICE_HOST=python-service
PYTHON_SERVICE_PORT=5000

# OpenAI
OPENAI_API_KEY=your_openai_api_key

# App
APP_NAME="AI Business Platform"
APP_ENV=production
APP_DEBUG=false
APP_URL=http://your-domain.com
APP_KEY=base64:your_app_key_here

# Mail
MAIL_MAILER=smtp
MAIL_HOST=your_smtp_host
MAIL_PORT=587
MAIL_USERNAME=your_smtp_username
MAIL_PASSWORD=your_smtp_password
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=your_email@domain.com
MAIL_FROM_NAME="${APP_NAME}" 